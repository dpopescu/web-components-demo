{
  "root": "libs/lit-web-components",
  "sourceRoot": "libs/lit-web-components/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/lit-web-components/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/lit-web-components"],
      "options": {
        "jestConfig": "libs/lit-web-components/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
